{
  "name": "rpghq-userscript-manager",
  "version": "3.0.2",
  "description": "RPGHQ Userscript Manager",
  "scripts": {
    "build": "npm run lint && node scripts/sort-manifest.js && node scripts/update-load-order.cjs && npx rollup -c && node scripts/inject-styles.js && prettier --write dist/rpghq-userscript-manager.user.js",
    "watch": "rollup -c -w",
    "lint": "prettier --write .",
    "add-script": "node tools/add-script.js",
    "outline": "node scripts/generate-outline.cjs",
    "remove-logs": "node scripts/remove-logging.js && npm run lint"
  },
  "author": "loregamer",
  "license": "MIT",
  "devDependencies": {
    "@babel/generator": "^7.27.0",
    "@babel/parser": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "glob": "^11.0.1",
    "inquirer": "^12.5.2",
    "micromatch": "^4.0.8",
    "prettier": "^3.5.3",
    "rollup": "^4.39.0",
    "rollup-plugin-import-css": "^3.5.1",
    "rollup-plugin-raw": "^0.0.1",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-userscript": "^0.3.7",
    "rollup-plugin-userscript-metablock": "^0.4.0"
  },
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loregamer/rpghq-userscripts.git"
  },
  "keywords": [],
  "type": "module",
  "bugs": {
    "url": "https://github.com/loregamer/rpghq-userscripts/issues"
  },
  "homepage": "https://github.com/loregamer/rpghq-userscripts#readme"
}
